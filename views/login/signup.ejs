<script src="//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
<link href="//maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<link rel='stylesheet' href='/stylesheets/login.css'/>
<!------ Include the above in your HEAD tag ---------->
<html lang="ko">
  <div class="wrapper fadeInDown">
    <div id="formContent">
        
      <div class="modal-header">
          <!-- Title -->
          <h5 class="modal-title" id="exampleModalLabel"> Sign Up </h5>
  
          <!-- X키 -->
          <button type="button" class="close" onclick="location.href='/'" >
              <span aria-hidden="true">&times;</span>
          </button>
      </div>
      <!-- Icon -->
      <div class="fadeIn first">
        <img src="/images/UIT_LOGO.png" id="icon" alt="User Icon" />
      </div>
      <!-- Login Form -->
      <form method="POST">
        <input type="text" id="name" class="fadeIn first" name="name" placeholder="이름">
        <input type="text" id="email" class="fadeIn second" name="email" placeholder="아이디">
        <input type="password" id="password" class="fadeIn third" name="pw" placeholder="비밀번호">
        <input type="submit" class="fadeIn fourth" value="회원가입">
      </form>
    </div>
  </div>
</html>

